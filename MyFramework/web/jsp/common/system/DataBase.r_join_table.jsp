<%@ page language="java" contentType="text/html;  charset=utf-8" pageEncoding="utf-8" session="true"%><%@ page	import="project.jun.dao.result.HoList"%><%@include file="/jsp/common/include/include.jsp"%><script>	function fs_view(url) {		document.join_form.target = "detail";		document.join_form.submit();	}	function fs_SetLeftTable(obj) {		var doc1 = document.join_form;		try {			if( doc1.left_table.length ) {				for( var i=0; i<doc1.left_table.length ; i++ ) {					doc1.left_table[i].value = obj.value;				}			} else {				doc1.left_table.value = obj.value;			}		} catch(E) {			alert(E);		}	}</script><span><form name="join_form" method="post" action="<%=G_CONTEXTROOT %>/system/dataBase.do" target="detail">	<input type="hidden" name="p_action_flag" value="r_join"></input>	<input type="hidden" name="owner" value="<%= param.get("owner")%>"></input>			* IN-LINE				<table border="1">					<colgroup>						<col width="20"/>						<col width="165"/>						<col width="165"/>					</colgroup>					<thead>						<tr>							<td style="text-align:center" >No</td>							<td style="text-align:center" >INLINE</td>							<td style="text-align:center" >AS TABLE</td>						</tr>					</thead>					<tbody>				<% for( int i=0 ; i<param.getValues("inline").length; i++ ) { %>						<tr>							<td><%= i+1 %></td>							<td>								<input type="hidden" name="inline" value="<%= param.getValues("inline")[i] %>"></input>								<%= param.getValues("inline")[i] %>							</td>							<td>								<select name="inline_src">									<option value="AA">AA</option>									<% for( int j=0 ; j<param.getValues("join").length; j++ ) { %>									<option value="<%= param.getValues("join")[j] %>"><%= param.getValues("join")[j] %></option>									<% } %>								</select>							</td>						</tr>				<% } %>					</tbody>				</table>				<br/>			* JOIN				<table border="1">					<colgroup>						<col width="20"/>						<col width="175"/>						<col width="175"/>					</colgroup>					<thead>						<tr>							<td style="text-align:center" >No</td>							<td style="text-align:center" >								LEFT TABLE<!-- br/>								<select name="left_table_master" onchange="fs_SetLeftTable(this)">									<option value="" selected="selected">Change LEFT TABLE to</option>									<% for( int j=0 ; j<param.getValues("join").length; j++ ) { %>									<option value="<%= param.getValues("join")[j] %>"><%= param.getValues("join")[j] %></option>									<% } %>								</select -->							</td>							<td style="text-align:center" >RIGHT TABLE</td>						</tr>					</thead>					<tbody>				<% for( int i=0 ; i<param.getValues("join").length; i++ ) { %>						<tr>							<td><%= i+1 %></td>							<td>								<select name="left_table">									<% for( int j=0 ; j<param.getValues("join").length; j++ ) { %>									<option value="<%= param.getValues("join")[j] %>" <%= i==j ? "selected=\"selected\"":"" %>><%= param.getValues("join")[j] %></option>									<% } %>									<option value="">-NONE-</option>								</select>								<select name="left_table_outer">									<option value=""></option>									<option value="(+)">(+)</option>								</select>							</td>							<td>								<select name="RIGHT_TABLE">									<% for( int j=0 ; j<param.getValues("join").length; j++ ) { %>									<option value="<%= param.getValues("join")[j] %>" <%= i+1==j ? "selected=\"selected\"":"" %>><%= param.getValues("join")[j] %></option>									<% } %>									<option value="" <%= i == param.getValues("join").length-1 ? "selected=\"selected\"":"" %>>-NONE-</option>								</select>								<select name="right_table_outer">									<option value=""></option>									<option value="(+)">(+)</option>								</select>							</td>						</tr>				<% } %>					</tbody>				</table>				*OPTION				<table border="1">					<colgroup>						<col width="350"/>					</colgroup>					<thead>						<tr>							<td style="text-align:center" >OPTION</td>						</tr>					</thead>					<tbody>						<tr>							<td>								<b>IN-LINE :</b><br/>								SELECT 절 - <select name="opt_sub_select">											<option value="SUB_ALL">모든 컬럼</option>											<option value="SUB_PK">PK만</option>											<option value="NONE" selected="selected">NONE</option>											<option value="COUNT">COUNT(*)</option>										</select><br/>										WHERE 절 - <select name="opt_sub">											<option value="PK">PK만</option>											<option value="ALL">모든 컬럼</option>											<option value="EQUAL" selected="selected">EQUAL</option>											<option value="NO_REGI">No Regi</option>										</select>							</td>						</tr>						<tr>							<td>								<b>JOIN :</b><br/>	JOIN 대상 - <select name="opt_join">											<option value="PK" selected="selected">PK만</option>											<option value="EQUAL">동일 컬럼모두</option>											<option value="NO_REGI">No Regi</option>										</select><br/>										WHERE 절- <select name="opt_join_where">											<option value="PK">PK만</option>											<option value="ALL" selected="selected">모든 컬럼</option>											<option value="NO_REGI">No Regi</option>										</select>							</td>						</tr>						<tr>							<td>								<input type="checkbox" name="cs" value="Y" id="comments_select" checked="checked"></input> <label for="comments_select">Comments SELECT</label>								<input type="checkbox" name="cp" value="Y" id="comments_padding"  ></input> <label for="comments_padding">(주석 넓이 같게)</label><br/>								<input type="checkbox" name="cf" value="Y" id="comments_from"  checked="checked"></input> <label for="comments_from">Comments FROM</label><br/>								<input type="checkbox" name="cw" value="Y" id="comments_where"  checked="checked"></input> <label for="comments_where">Comments WHERE</label><br/>								<input type="text" name="cil" size="2" id="Column" value="" /> <label for="Column">컬럼씩 라인당 출력</label>							</td>						</tr>						<tr>							<td>								<input type="radio" name="param_type" value="M" id="manual" checked="checked"></input> <label for="manual">Manual(공백)</label>							</td>						</tr>						<tr>							<td>								<input type="radio" name="param_type" value="P" id="p"  ></input> <label for="p">:P (WizFw 3.3)</label>								<input type="radio" name="param_type" value="PN" id="p_p"  ></input> <label for="p_p">:P[N] (WizFw 3.3)</label>							</td>						</tr>						<tr>							<td>								<input type="radio" name="param_type" value="I" id="ibatis"  ></input> <label for="ibatis">IBATIS</label>								<input type="radio" name="param_type" value="My" id="mybatis"></input> <label for="ibatis">MyBATIS</label>								<input type="checkbox" name="vo" value="Y" id="comments_vo"></input> <label for="comments_vo">VO</label>								<br/>								Where절 생성조건 : 								<input type="checkbox" name="condition" value="IF" id="equals"></input> <label for="equals">If</label>								<input type="checkbox" name="condition_each" value="EACH" id="forEach"></input> <label for="forEach">ForEach</label>							</td>						</tr>					</tbody>				</table><input type="submit" value="Make Query"></input></form></span>